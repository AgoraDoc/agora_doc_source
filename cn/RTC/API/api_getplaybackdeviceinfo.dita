<reference id="api_getplaybackdeviceinfo">
    <title><ph keyref="getPlaybackDeviceInfo"></ph></title>
    <shortdesc>根据设备 ID 和设备名称获取播放设备。</shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="getPlaybackDeviceInfo" />
            </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        <title>原型</title>
        <p conref="../conref/conref_rtc_api.dita#apidef/getPlaybackDeviceInfo"/>
        </section>
        <section id="parameters"><title>参数</title>
            <parml>
                <plentry>
                    <pt>deviceId</pt>
                    <pd>播放设备的设备 ID。</pd>
                </plentry>
                <plentry>
                    <pt>deviceName</pt>
                    <pd>播放设备的设备名称。</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>返回值</title>
            <ul>
                <li>0: 方法调用成功。</li>
                <li>&lt; 0: 方法调用失败。</li>
            </ul>
        </section></refbody>
</reference>
